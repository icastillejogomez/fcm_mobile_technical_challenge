    "@types/react-test-renderer": "18.0.7",
    "eslint": "8.57.0",
    "eslint-config-expo": "7.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "jest": "29.6.3",
    "jest-expo": "51.0.1",
    "prettier": "3.3.2",
    "react-test-renderer": "18.2.0",
    "typescript": "5.3.3"
