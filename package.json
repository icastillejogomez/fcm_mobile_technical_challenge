    "expo-constants": "16.0.2",
    "expo-font": "12.0.9",
    "expo-image": "1.12.12",
    "expo-linking": "6.3.1",
    "expo-router": "3.5.18",
    "expo-splash-screen": "0.27.5",
    "expo-status-bar": "1.12.1",
    "graphql": "15.8.0",
    "json-graphql-server": "2.3.2",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.16.1",
